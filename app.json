{
  "expo": {
    "name": "portfolio",
    "slug": "portfolio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "title": "Achyuth KP | Full Stack Engineer",
      "description": "Portfolio of Achyuth KP, Full Stack Engineer specializing in React, React Native, and modern tech.",
      "themeColor": "#2563eb",
      "meta": {
        "google-site-verification": "generic-verification-code",
        "og:description": "Portfolio of Achyuth KP. View my projects and skills.",
        "og:title": "Achyuth KP | Full Stack Engineer"
      }
    },
    "experiments": {
      "baseUrl": "/cross-platform-portfolio"
    },
    "plugins": []
  }
}